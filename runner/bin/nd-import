#!/bin/bash
#
# Nora Docker
#
cd $(dirname $0)/..;
source ./config/vars

NAME=$1
shift;

[ "$NAME" == "" ] && exit 1

for file in $PATH_LIST
do
    [ "$NAME" == "$(basename $file)" ] && {
    INSTANCE=$($file name)
        cat - | docker run -i --rm --volumes-from $INSTANCE busybox sh -c 'cat - | tar xvf -'
       exit 0
    }
done
